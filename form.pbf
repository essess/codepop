Global Window_codepop

Global Button_getcodes, ListView_codes, Combo_ports

Declare Button_getcodes_Events(EventType)

Procedure OpenWindow_codepop(x = 0, y = 0, width = 170, height = 220)
  Window_codepop = OpenWindow(#PB_Any, x, y, width, height, "", #PB_Window_SystemMenu | #PB_Window_ScreenCentered)
  Button_getcodes = ButtonGadget(#PB_Any, 95, 180, 65, 30, "Get Codes")
  ListView_codes = ListViewGadget(#PB_Any, 10, 40, 150, 130)
  Combo_ports = ComboBoxGadget(#PB_Any, 10, 10, 150, 20)
  DisableGadget(Combo_ports, 1)
EndProcedure

Procedure Window_codepop_Events(event)
  Select event
    Case #PB_Event_CloseWindow
      ProcedureReturn #False

    Case #PB_Event_Menu
      Select EventMenu()
      EndSelect

    Case #PB_Event_Gadget
      Select EventGadget()
        Case Button_getcodes
          Button_getcodes_Events(EventType())          
      EndSelect
  EndSelect
  ProcedureReturn #True
EndProcedure


